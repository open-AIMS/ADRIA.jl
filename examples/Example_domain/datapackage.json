    "data": [
        "reef_siteid",
        "dhw"
    ],
    "path": "DHWs/dhwRCP45.nc",
    "format": "netCDF"
},
{
    "name": "waves",
    "description": "Stochastic wave data for the Example cluster",
    "data": [
        "reef_siteid",
        "Ub"
    ],
    "path": "waves/wave_RCP45.nc",
    "format": "netCDF"
},
{
    "name": "cyclones",
    "description": "Empty cyclone data for the Example cluster",
    "data": [
        "reef_siteid",
        "layer"
    ],
